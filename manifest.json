{
  "manifest_version": 3,
  "name": "Latex Copy Helper for Word",
  "version": "1.0.0",
  "description": "Add a draggable floating button to copy selected text and normalize LaTeX for Word.",
  "permissions": [
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_idle"
    }
  ]
}
